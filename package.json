{
  "name": "villager-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "chess.js": "^0.12.0",
    "react": "^18.2.0",
    "react-chessboard": "^2.1.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "tournament-pairings": "^1.5.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "build": "react-scripts build",
    "test": "jest ./src",
    "eject": "react-scripts eject",
    "tsc:check": "tsc --noEmit",
    "tsc:check-app": "tsc --noEmit --project \"./tsconfig.json\"",
    "tsc:check-cypress": "tsc --noEmit --project \"./cypress/tsconfig.json\"",
    "lint": "eslint \"./{src, cypress}/**/*.{js, jsx, ts, tsx, json}\"",
    "cypress:open": "npx cypress open --config-file cypress/cypress.config.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.1",
    "cypress": "^13.7.0"
  }
}
